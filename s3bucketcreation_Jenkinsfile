pipeline {
    agent any

    environment {
        AWS_DEFAULT_REGION = 'us-east-1'
        BUCKET_NAME = 'bucketjenkinsawss37838621221'
    }

    stages {
        stage('Create S3 Bucket') {
            steps {
                sh """
                    aws s3api create-bucket \
                    --bucket $BUCKET_NAME \
                    --region $AWS_DEFAULT_REGION
                """
            }
        }
    }
}
